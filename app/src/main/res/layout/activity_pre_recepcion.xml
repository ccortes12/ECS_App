<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Recepcion.Pre_recepcion">

    <LinearLayout
        android:layout_width="@dimen/largo_cuadros"
        android:layout_height="126dp"
        android:layout_marginTop="@dimen/margen_top"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView74">

        <Button
            android:id="@+id/button_recepcion1"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:text="Recepción lote"
            android:textSize="18sp" />

        <Button
            android:id="@+id/button_recepcionManual"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:text="Recepción manual"
            android:textSize="18sp" />

    </LinearLayout>

    <TextView
        android:id="@+id/textView74"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="Recepción"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>